{
  "name": "node-js-getting-started",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "node --max-http-header-size 80000 build/index.js",
    "postinstall": "tsc && npm run buildClient",
    "tsc": "tsc",
    "buildClient": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "dependencies": {
    "@asciidoctor/reveal.js": "^4.0.1",
    "asciidoctor": "^2.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "4.15.2",
    "express-async-wrap": "^1.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sha.js": "^2.4.11",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@types/express": "4.17.2",
    "@types/express-async-wrap": "^1.0.0",
    "@types/node": "^12.12.7",
    "@types/request-promise": "^4.1.44",
    "@types/sha.js": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-js-getting-started"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
